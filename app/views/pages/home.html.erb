  <!-- MY EXPERIENCES -->
  <h2 class="titles">My experiences </h2>
  <div class="container container_exp">
    <div class="row">
      <% @experiences.each do |experience| %>
        <div class="col-sm-12 col-md-6 col-lg-4">
          <article class="card_exp">
            <div class="thumb_exp"></div>
            <div class="infos_exp">
              <h2 class="title_exp"><%= experience.role%><span class="flag_exp"></span></h2>
              <h3 class="date_exp"><%= experience.datebegin %></h3>
              <h3 class="seats_exp"><%= experience.company %></h3>
              <p class="txt_exp">
              <%= experience.description.slice(0..200) %>...
              </p>
              <h3 class="button_more" data-toggle="modal" data-target="<%="#exampleModalLong-#{experience.id}"%>">more...</h3>
              <h3 class="details_exp">event details</h3>
            </div>
          </article>
        </div>

        <div class="modal fade" id="<%="exampleModalLong-#{experience.id}"%>" tabindex="-1" role="dialog" aria-labelledby="exampleModalLongTitle" aria-hidden="true">
          <div class="modal-dialog" role="document">
            <div class="modal-content">
              <div class="modal-header">
                <h5 class="modal-title" id="exampleModalLongTitle"><%=experience.company %></h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                  <span aria-hidden="true">&times;</span>
                </button>
              </div>
              <div class="modal-body">
                <%= experience.description %>
              </div>
              <div class="modal-footer">
                <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                <button type="button" class="btn btn-primary">Save changes</button>
              </div>
            </div>
          </div>
        </div>
      <% end %>
    </div>
  </div>

